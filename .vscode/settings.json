{
  "emeraldwalk.runonsave": {
    "commands": [
      {
        "match": ".*",
        "isAsync": true,
        "cmd": "npx tailwindcss -i ./css/tailwind_src.css -o ./css/tailwind_dist.css && make"
      }
    ]
  },
  "liveServer.settings.ignoreFiles" : [
    "css/site_dist.css",
    "css/site_src.css"
  ]
} 
