{
    "rules": {
        "no-console": [
            0
        ],
        "quotes": [
            2,
            "single"
        ],
        "linebreak-style": [
            2,
            "unix"
        ],
        "semi": [
            2,
            "always"
        ],
        "no-unused-vars": [
            0
        ],
        "indent": [
            "error",
            2
        ]
    },
    "env": {
        "es6": true,
        "node": true,
        "browser": true
    },
    "globals": {
        "mapboxgl": true,
        "MapboxGeocoder": true,
        "MapboxDraw": true,
        "turf": true,
        "CodeMirror": true,
        "Base64": true,
        "d3": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 9
    }
}